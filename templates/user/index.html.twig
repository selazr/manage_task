{% extends 'base.html.twig' %}

{% block title %}Llista d'Usuaris{% endblock %}

{% block body %}
    <h1>Llista d'Usuaris</h1>
    <ul>
        {% for user in users %}
            <li>
                <a href="{{ path('app_user_show', {'id': user.id}) }}">
                    {{ user.email }}
                </a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}